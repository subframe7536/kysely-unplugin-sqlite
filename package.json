{
  "name": "kysely-unplugin-sqlite",
  "type": "module",
  "version": "0.1.3",
  "description": "Unplugin for Kysely and SQLite to optimize bundled size",
  "author": "subframe7536",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subframe7536/kysely-unplugin.git"
  },
  "keywords": [
    "kysely",
    "sqlite",
    "database",
    "unplugin"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "format": "eslint --fix",
    "test": "bun example/build.ts --dry",
    "release": "bun example/build.ts --clean && bun run build && bumpp --all && npm publish"
  },
  "peerDependencies": {
    "kysely": ">=0.28",
    "magic-string": "^0.30.17",
    "unplugin": "^2.3.0"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^1.2.3",
    "@subframe7536/type-utils": "^0.1.6",
    "@types/node": "^22.14.1",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "kysely": "^0.28.0",
    "kysely-wasm": "^1.1.0",
    "magic-string": "^0.30.17",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "unplugin": "^2.3.2"
  }
}
